{
    "_config_name": "views.view.ckeditor_mentions",
    "name": "ckeditor_mentions",
    "description": "List of users",
    "tag": "default",
    "disabled": false,
    "base_table": "users",
    "human_name": "CKEditor mentions",
    "core": 7,
    "display": {
        "default": {
            "display_title": "Master",
            "display_plugin": "default",
            "display_options": {
                "query": {
                    "type": "views_query",
                    "options": []
                },
                "use_more_always": false,
                "access": {
                    "type": "perm",
                    "perm": "access user profiles"
                },
                "cache": {
                    "type": "none"
                },
                "exposed_form": {
                    "type": "basic"
                },
                "pager": {
                    "type": "some",
                    "options": {
                        "items_per_page": "10"
                    }
                },
                "style_plugin": "list",
                "style_options": {
                    "default_row_class": false,
                    "row_class_special": false
                },
                "row_plugin": "fields",
                "row_options": {
                    "default_field_elements": false
                },
                "fields": {
                    "uid": {
                        "id": "uid",
                        "table": "users",
                        "field": "uid",
                        "label": "",
                        "exclude": true,
                        "element_label_colon": false,
                        "link_to_user": false
                    },
                    "name": {
                        "id": "name",
                        "table": "users",
                        "field": "name",
                        "label": "",
                        "alter": {
                            "alter_text": true,
                            "text": "<a class=\"mention-users\" data-uid=\"[uid]\" data-realname=\"[name]\">[name]</a>",
                            "word_boundary": false,
                            "ellipsis": false
                        },
                        "element_label_colon": false,
                        "link_to_user": false
                    }
                },
                "filters": {
                    "status": {
                        "id": "status",
                        "table": "users",
                        "field": "status",
                        "value": "1",
                        "group": 1,
                        "expose": {
                            "operator": false
                        }
                    },
                    "realname": {
                        "id": "realname",
                        "table": "realname",
                        "field": "realname",
                        "operator": "starts",
                        "expose": {
                            "operator_id": "realname_op",
                            "operator": "realname_op",
                            "identifier": "realname"
                        }
                    }
                }
            }
        },
        "block": {
            "display_title": "Block",
            "display_plugin": "block",
            "display_options": {
                "query": {
                    "type": "views_query",
                    "options": []
                },
                "defaults": {
                    "hide_admin_links": false
                }
            }
        }
    }
}